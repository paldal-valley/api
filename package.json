{
  "name": "ajou-coin-api",
  "version": "0.0.0",
  "description": "ajou-coin api server",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/app.js",
    "serve": "cross-env NODE_ENV=production nodemon --exec babel-node src/app.js",
    "test": "node ./node_modules/nyc/bin/nyc.js cross-env NODE_ENV=test node_modules/mocha/bin/_mocha \"./{,!(node_modules)/**/}*.spec.js\"  --recursive --require @babel/register --exit",
    "unit": "npm test -- -g",
    "cover": "node ./node_modules/nyc/bin/nyc.js report",
    "coveralls": "npm run cover -- --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paldal-valley/ajou-coin.git"
  },
  "author": "paldalvalley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paldal-valley/ajou-coin/issues"
  },
  "homepage": "https://github.com/paldal-valley/ajou-coin#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "nodemailer": "^5.1.1",
    "slack-node": "^0.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.3",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  }
}
